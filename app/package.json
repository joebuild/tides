{
  "name": "tides",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "check": "svelte-check --tsconfig tsconfig.json",
    "check:watch": "svelte-check --tsconfig tsconfig.json --watch",
    "dev": "vite dev",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "preview": "vite preview",
    "start": "ts-node -T src/start.ts",
    "clean": "rm -rf ./dist && rm -rf ./build",
    "build:infra": "npm run esbuild",
    "zip": "cd dist && zip -q -r lambda.zip lambda.js lambda.js.map",
    "dist": "npm run build:infra && npm run zip",
    "dist:python": "cd infrastructure/python && rm -rf ./package && rm -f package.zip && mkdir ./package && pip install --target ./package -r requirements.txt && cd package && zip -r ../package.zip . && cd .. && zip -g package.zip Lambda.py && cd ../..",
    "esbuild": "esbuild --bundle --minify --keep-names --sourcemap --sources-content=false --target=node14 --platform=node --outfile=dist/lambda.js src/infrastructure/lambda.ts",
    "deploy": "aws lambda update-function-code --function-name MyLambdaFunction --zip-file fileb://dist/lambda.zip --profile default",
    "stack": "npm run dist && npm run dist:python && cd infrastructure && npx cdk deploy --all"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.178.0",
    "@aws-sdk/client-sqs": "^3.178.0",
    "@aws-sdk/client-timestream-query": "^3.165.0",
    "@aws-sdk/client-timestream-write": "^3.165.0",
    "@aws-sdk/lib-dynamodb": "^3.178.0",
    "@babel/runtime": "7.18.9",
    "@poppanator/sveltekit-svg": "^0.3.4",
    "@project-serum/anchor": "0.25.0",
    "@pythnetwork/client": "^2.8.0",
    "@rollup/plugin-inject": "^4.0.4",
    "@solana/buffer-layout": "4.0.0",
    "@solana/spl-token": "0.2.0",
    "@solana/spl-token-registry": "^0.2.4574",
    "@solana/wallet-adapter-base": "0.9.16",
    "@solana/wallet-adapter-ledger": "^0.9.14",
    "@solana/wallet-adapter-wallets": "^0.18.7",
    "@solana/web3.js": "^1.53.0",
    "@svelte-on-solana/wallet-adapter-anchor": "^1.0.16-alpha.0",
    "@svelte-on-solana/wallet-adapter-core": "^1.0.8-alpha.0",
    "@svelte-on-solana/wallet-adapter-ui": "^1.0.20-alpha.0",
    "@sveltejs/adapter-auto": "^1.0.0-next.64",
    "@tailwindcss/typography": "^0.5.4",
    "async": "^3.2.3",
    "autoprefixer": "^10.4.8",
    "axios": "^0.27.2",
    "bigint-buffer": "1.1.5",
    "bn.js": "^5.2.0",
    "bs58": "^5.0.0",
    "buffer": "^6.0.3",
    "daisyui": "^2.22.0",
    "decimal.js": "^10.3.1",
    "dotenv": "^16.0.1",
    "eventemitter3": "^4.0.7",
    "lightweight-charts": "^3.8.0",
    "p-limit": "^4.0.0",
    "process": "^0.11.10",
    "rollup": "^2.77.3",
    "svelte-frappe-charts": "^1.9.2",
    "svelte-lightweight-charts": "^1.7.0-next.0",
    "svelte-loading-spinners": "^0.1.7",
    "tailwindcss": "^3.1.8",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@esbuild-plugins/node-globals-polyfill": "^0.1.1",
    "@sveltejs/adapter-static": "^1.0.0-next.42",
    "@sveltejs/kit": "^1.0.0-next.480",
    "@types/aws-lambda": "^8.10.102",
    "@types/bn.js": "^5.1.0",
    "@types/node": "^18.7.1",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "babel-loader": "^8.1.0",
    "esbuild": "^0.15.1",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "prettier": "^2.6.2",
    "prettier-plugin-svelte": "^2.4.0",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "svelte": "^3.50.0",
    "svelte-check": "^2.2.6",
    "svelte-preprocess": "^4.9.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.6.3",
    "vite": "^3.0.0-alpha.14"
  }
}
